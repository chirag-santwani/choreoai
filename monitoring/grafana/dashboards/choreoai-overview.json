{
  "dashboard": {
    "id": null,
    "uid": "choreoai-overview",
    "title": "ChoreoAI Overview",
    "tags": ["choreoai", "api", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(choreoai_requests_total[5m])",
            "legendFormat": "{{provider}} - {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(choreoai_errors_total[5m])",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Errors/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Request Duration (P50, P95, P99)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(choreoai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(choreoai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(choreoai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "choreoai_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          }
        ]
      },
      {
        "id": 5,
        "title": "Token Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(choreoai_tokens_total{type=\"input\"}[5m])",
            "legendFormat": "Input - {{provider}}",
            "refId": "A"
          },
          {
            "expr": "rate(choreoai_tokens_total{type=\"output\"}[5m])",
            "legendFormat": "Output - {{provider}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Tokens/sec"
          }
        ]
      },
      {
        "id": 6,
        "title": "Requests by Provider",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum by (provider) (rate(choreoai_requests_total[5m]))",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "(sum(rate(choreoai_requests_total{status=~\"2..\"}[5m])) / sum(rate(choreoai_requests_total[5m]))) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "unit": "percent"
        }
      },
      {
        "id": 8,
        "title": "Total Requests (24h)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(increase(choreoai_requests_total[24h]))",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Streaming Requests",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(rate(choreoai_streaming_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "unit": "reqps"
        }
      },
      {
        "id": 10,
        "title": "Total Errors (24h)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(increase(choreoai_errors_total[24h]))",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
